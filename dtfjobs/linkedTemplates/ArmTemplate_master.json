{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dtfjobs"
		},
		"JOBSDL_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'JOBSDL'"
		},
		"JOBSQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'JOBSQL'"
		},
		"JOBSDL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://jobscloud.dfs.core.windows.net"
		},
		"Web_Source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.fundamentus.com.br/resultado.php/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dtfjobs_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"JOBSDL_accountKey": {
						"value": "[parameters('JOBSDL_accountKey')]"
					},
					"JOBSQL_connectionString": {
						"value": "[parameters('JOBSQL_connectionString')]"
					},
					"JOBSDL_properties_typeProperties_url": {
						"value": "[parameters('JOBSDL_properties_typeProperties_url')]"
					},
					"Web_Source_properties_typeProperties_url": {
						"value": "[parameters('Web_Source_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}